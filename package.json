{
  "name": "@subframe/core",
  "version": "1.128.0",
  "description": "",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "build": "rm -rf dist/ && npm run build:icons && npm run build:rollup",
    "build:rollup": "rollup -c",
    "build:types": "tsc -p tsconfig.types.json",
    "build:icons": "rm -rf src/assets/icons/final/* && npx @svgr/cli --filename-case kebab --index-template index-template.js --icon --typescript -d ./src/assets/icons/final ./src/assets/icons/raw --no-svgo",
    "dev": "tsc --watch",
    "update-version": "npm version minor",
    "build-for-publish": "npm run build",
    "publish-to-npm": "npm publish --access public",
    "minor-publish": "npm run build && npm version minor && npm publish --access public",
    "ts": "tsc --noEmit",
    "ts:watch": "tsc --watch --incremental --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Irvin Zhan",
  "license": "ISC",
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "babel-jest": "^29.7.0",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.21",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "typescript": "^5.1.3",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.2",
    "@radix-ui/react-context": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-hover-card": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.2",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "1.0.7",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "react-day-picker": "^9.0.4",
    "react-virtualized-auto-sizer": "^1.0.20",
    "recharts": "^2.10.3",
    "rollup-plugin-postcss": "^4.0.2",
    "sonner": "^0.7.0",
    "tailwind-merge": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  }
}
